<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1080.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="financelerui.getAccountui">
	<children>
		<ImageView fitHeight="800.0" fitWidth="1080.0">
			<image>
				<Image url="@../background.jpg" />
			</image>
		</ImageView>
		<Button layoutX="1007.0" layoutY="5.0" maxHeight="30.0"
			maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false"
			onAction="#HandleHelp" prefHeight="30.0" prefWidth="30.0"
			style="-fx-background-color: null;">
			<graphic>
				<ImageView fitHeight="50.0" fitWidth="50.0">
					<image>
						<Image url="@../question.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button layoutX="1042.0" layoutY="5.0" maxHeight="30.0"
			maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false"
			onAction="#HandleOut" prefHeight="30.0" prefWidth="30.0"
			style="-fx-background-color: null;">
			<graphic>
				<ImageView fitHeight="50.0" fitWidth="50.0">
					<image>
						<Image url="@../home.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button layoutY="150.0" mnemonicParsing="false" onAction="#HandleEsc"
			style="-fx-background-color: null;">
			<graphic>
				<ImageView fitHeight="70.0" fitWidth="50.0">
					<image>
						<Image url="@../09-876.png" />
					</image>
				</ImageView>
			</graphic>
		</Button>
		<Button layoutX="390.0" layoutY="743.0" mnemonicParsing="false"
			onAction="#HandleSure" text="确认" />
		<Button layoutX="602.0" layoutY="742.0" mnemonicParsing="false"
			onAction="#HandleCancle" prefHeight="32.0" prefWidth="57.0" text="取消" />
		<Text layoutX="500.0" layoutY="245.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="查看银行账户" wrappingWidth="138.0">
			<font>
				<Font size="20.0" />
			</font>
		</Text>
		<Label layoutX="750.0" layoutY="230.0" text="搜索">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
